import maya.cmds as cmds

def poly_Off():
    myPanel = cmds.getPanel(underPointer=True)
    cmds.modelEditor(myPanel, edit = True,polymeshes = False)

poly_Off()






import maya.cmds as cmds

def poly_ON():
    myPanel = cmds.getPanel(underPointer=True)
    cmds.modelEditor(myPanel, edit = True,polymeshes = True)

poly_ON()

